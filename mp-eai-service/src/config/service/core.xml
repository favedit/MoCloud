<?xml version="1.0" encoding="UTF-8"?>

<Config>

   <!-- ============================================================ -->
   <!-- 时间控制台                                                   -->
   <!-- ============================================================ -->
   <Component
         face   = 'org.mo.cloud.common.datetime.IDateTimeConsole'
         class  = 'org.mo.cloud.common.datetime.FDateTimeConsole'
         scope  = 'global'
         deploy = 'start'>
      <Initialize name='initialize'/>
   </Component>

   <!-- ============================================================ -->
   <!-- GcLoggerConsole: 日志控制台                                  -->
   <!-- ============================================================ -->
   <Component
         face   = 'org.mo.cloud.core.logger.IGcLoggerConsole'
         class  = 'org.mo.cloud.core.logger.FGcLoggerConsole'
         scope  = 'global'
         deploy = 'start'>
      <Property name='enable'>${cloud.server.logger.enable}</Property>
      <Property name='server_code'>${application.code}</Property>
      <Property name='logger_interval'>10000</Property>
      <Property name='logger_service_publish'>${cloud.server.logger.publish}</Property>
      <Initialize name='initialize'/>
   </Component>

   <!-- ============================================================ -->
   <!-- FinancialConsole:金融控制台                                  -->
   <!--  I:initialize - 初始化                                       -->
   <!-- ============================================================ -->
   <Component
         face  = 'com.ahyc.eai.core.financial.IFinancialConsole'
         class = 'com.ahyc.eai.core.financial.FFinancialConsole'
         scope = 'global'>
      <Initialize name='initialize'/>
   </Component>
   
   <!-- ==================================================== -->
   <!-- StorageConsole:资源存储控制台                        -->
   <!-- ==================================================== -->
   <Component
         face  = 'org.mo.content.core.mobile.account.ILoginConsole'
         class = 'org.mo.content.core.mobile.account.FLoginConsole'
         scope = 'global'>
      <Property name='oaLoginEnable'>Y</Property>
      <!-- <Property name='oaLoginUrl'>http://10.13.0.141:6666/ycjt/auth/auth.jsp</Property> -->
       <Property name='oaLoginUrl'>http://115.182.232.150:8181/oauser</Property>
   </Component>
</Config>
